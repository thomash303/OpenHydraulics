within OpenHydraulics.Custom.Valves;

package ValveCharacteristics
  partial function baseValveCharacteristic
    "Package containg the valve characteristic functions, modified from the MSL fluid library"
      input Real openingNomimal(min=0, max=1)
          "Opening position (0: closed, 1: fully open)";
      output Real openingFraction "Relative flow coefficient (per unit)";
  end baseValveCharacteristic;
end ValveCharacteristics;
